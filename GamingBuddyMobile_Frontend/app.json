{
  "expo": {
    "name": 'Fixer',
    "slug": 'Fixer',
    "version": '1.0.0',
    "orientation": 'portrait',
    "icon": './assets/icon.png',
    "splash": {
      "image": './assets/splash.png',
      "resizeMode": 'contain',
      "backgroundColor": '#ffffff'
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": './src/assets/favicon.png'
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ]
  }
}